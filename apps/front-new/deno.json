{
  "name": "front-new",
  "version": "1.0.0",
  "exports": "./src/main.tsx",
  "tasks": {
    "dev": "deno run -A npm:vite -- --port 3000",
    "start": "deno run --allow-net --allow-read --allow-env --allow-write npm:vite -- --port 3000",
    "build": "deno run --allow-net --allow-read --allow-env --allow-write npm:vite -- build && deno run --allow-read --allow-write npm:tsc",
    "serve": "deno run --allow-net --allow-read --allow-env --allow-write npm:vite -- preview",
    "test": "deno run --allow-net --allow-read --allow-env --allow-write npm:vitest -- run",
    "lint": "deno run --allow-net --allow-read --allow-env --allow-write npm:eslint",
    "format": "deno run --allow-net --allow-read --allow-env --allow-write npm:prettier",
    "check": "deno run --allow-net --allow-read --allow-env --allow-write npm:prettier -- --write . && deno run --allow-net --allow-read --allow-env --allow-write npm:eslint -- --fix"
  },
  "imports": {
    "@chakra-ui/react": "npm:@chakra-ui/react@^3.22.0",
    "@emotion/react": "npm:@emotion/react@^11.14.0",
    "@faker-js/faker": "npm:@faker-js/faker@^9.6.0",
    "@tanstack/match-sorter-utils": "npm:@tanstack/match-sorter-utils@^8.19.4",
    "@tanstack/react-form": "npm:@tanstack/react-form@^1.0.0",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.66.5",
    "@tanstack/react-query-devtools": "npm:@tanstack/react-query-devtools@^5.66.5",
    "@tanstack/react-router": "npm:@tanstack/react-router@^1.121.2",
    "@tanstack/react-router-devtools": "npm:@tanstack/react-router-devtools@^1.121.2",
    "@tanstack/react-table": "npm:@tanstack/react-table@^8.21.2",
    "@tanstack/router-plugin": "npm:@tanstack/router-plugin@^1.121.2",
    "chakra-react-select": "npm:chakra-react-select@^6.1.0",
    "next-themes": "npm:next-themes@^0.4.6",
    "react": "npm:react@^19.0.0",
    "react-dom": "npm:react-dom@^19.0.0",
    "@tanstack/eslint-config": "npm:@tanstack/eslint-config@^0.1.0",
    "@testing-library/dom": "npm:@testing-library/dom@^10.4.0",
    "@testing-library/react": "npm:@testing-library/react@^16.2.0",
    "@types/react": "npm:@types/react@^19.0.8",
    "@types/react-dom": "npm:@types/react-dom@^19.0.3",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "jsdom": "npm:jsdom@^26.0.0",
    "prettier": "npm:prettier@^3.5.3",
    "react-icons": "npm:react-icons@^5.5.0",
    "typescript": "npm:typescript@^5.7.2",
    "vite": "npm:vite@^6.1.0",
    "vitest": "npm:vitest@^3.0.5",
    "web-vitals": "npm:web-vitals@^4.2.4",
    "zod": "npm:zod@^4.0.3"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "eslint.config.js",
    "prettier.config.js",
    "vite.config.js"
  ],
  "compilerOptions": {
    "target": "ES2022",
    "jsx": "react-jsx",
    "module": "ESNext",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "types": ["vite/client"],
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
