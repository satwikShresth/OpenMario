{
   "name": "@openmario/front",
   "version": "1.0.0",
   "exports": "./src/main.tsx",
   "tasks": {
      "gen:client": "deno run -A @hey-api/openapi-ts -f ./openapi.config.ts",
      "dev:watch": "deno run --env-file=../../.env -A --unstable-no-legacy-abort npm:vite",
      "dev": "deno run -A --env-file=../../.env --unstable-no-legacy-abort npm:vite",
      "build": "deno run -A npm:vite build",
      "serve": "deno run --allow-net --allow-read --allow-env --allow-write npm:vite -- preview",
      "test": "deno run --allow-net --allow-read --allow-env --allow-write npm:vitest -- run",
      "check": "deno run --allow-net --allow-read --allow-env --allow-write npm:prettier -- --write . && deno run --allow-net --allow-read --allow-env --allow-write npm:eslint -- --fix"
   },
   "imports": {
      "@chakra-ui/react": "npm:@chakra-ui/react@^3.22.0",
      "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.5",
      "@emotion/react": "npm:@emotion/react@^11.14.0",
      "@faker-js/faker": "npm:@faker-js/faker@^9.6.0",
      "@hey-api/openapi-ts": "npm:@hey-api/openapi-ts@^0.78.3",
      "@hey-api/vite-plugin": "npm:@hey-api/vite-plugin@^0.2.0",
      "@tanstack/match-sorter-utils": "npm:@tanstack/match-sorter-utils@^8.19.4",
      "@tanstack/react-form": "npm:@tanstack/react-form@^1.14.1",
      "@tanstack/react-query": "npm:@tanstack/react-query@^5.66.5",
      "@tanstack/react-query-devtools": "npm:@tanstack/react-query-devtools@^5.66.5",
      "@tanstack/react-router": "npm:@tanstack/react-router@^1.128.0",
      "@tanstack/react-router-devtools": "npm:@tanstack/react-router-devtools@^1.128.0",
      "@tanstack/react-table": "npm:@tanstack/react-table@^8.21.2",
      "@tanstack/router-plugin": "npm:@tanstack/router-plugin@^1.128.0",
      "chakra-react-select": "npm:chakra-react-select@^6.1.0",
      "immer": "npm:immer@^10.1.1",
      "jsurl2": "npm:jsurl2@^2.2.0",
      "next-themes": "npm:next-themes@^0.4.6",
      "posthog-js": "npm:posthog-js@^1.257.1",
      "react": "npm:react@^19.0.0",
      "react-dom": "npm:react-dom@^19.0.0",
      "@tanstack/eslint-config": "npm:@tanstack/eslint-config@^0.2.0",
      "@testing-library/dom": "npm:@testing-library/dom@^10.4.0",
      "@testing-library/react": "npm:@testing-library/react@^16.2.0",
      "@types/react": "npm:@types/react@^19.0.8",
      "@types/react-dom": "npm:@types/react-dom@^19.0.3",
      "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
      "jsdom": "npm:jsdom@^26.0.0",
      "prettier": "npm:prettier@^3.5.3",
      "react-icons": "npm:react-icons@^5.5.0",
      "tesseract.js": "npm:tesseract.js@^6.0.1",
      "typescript": "npm:typescript@^5.7.2",
      "vite": "npm:vite@^7.0.5",
      "vite-plugin-babel": "npm:vite-plugin-babel@^1.3.2",
      "vitest": "npm:vitest@^3.0.5",
      "web-vitals": "npm:web-vitals@^5.0.3",
      "zod": "npm:zod@^4.0.5",
      "@/": "./src/",
      "zustand": "npm:zustand@^5.0.6",
      "zustand-context": "npm:zustand-context@^1.0.0"
   },
   "include": [
      "**/*.ts",
      "**/*.tsx",
      "eslint.config.js",
      "prettier.config.js",
      "vite.config.js"
   ],
   "compilerOptions": {
      "target": "ES2022",
      "jsx": "react-jsx",
      "module": "ESNext",
      "lib": ["ES2022", "DOM", "DOM.Iterable"],
      "types": ["vite/client"],
      "moduleResolution": "bundler",
      "allowImportingTsExtensions": true,
      "verbatimModuleSyntax": true,
      "noEmit": true,
      "skipLibCheck": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noFallthroughCasesInSwitch": true,
      "noUncheckedSideEffectImports": true,
      "esModuleInterop": true,
      "baseUrl": ".",
      "types": [
         "react",
         "react-dom",
         "@types/react"
      ],
      "lib": [
         "dom",
         "dom.iterable",
         "deno.ns"
      ],
      "jsxImportSource": "react",
      "paths": {
         "@/*": ["./src/*"]
      }
   }
}
